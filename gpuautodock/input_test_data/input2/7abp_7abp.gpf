npts 60 60 60                        # num.grid points in xyz

gridfld 7abp_rec.maps.fld            # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C HD N NA OA SA     # receptor atom types
ligand_types C HD OA                 # ligand atom types
receptor 7abp_rec.pdbqt              # macromolecule
gridcenter 13.3089 56.8256 53.8787   # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 7abp_rec.C.map                   # atom-specific affinity map
map 7abp_rec.HD.map                  # atom-specific affinity map
map 7abp_rec.OA.map                  # atom-specific affinity map
elecmap 7abp_rec.e.map               # electrostatic potential map
dsolvmap 7abp_rec.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
